{
  "name": "comercialprado",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --colors",
    "build": "rimraf release && NODE_ENV=production webpack",
    "test": "npm run lint && jest --coverage",
    "test:watch": "npm run lint && jest --watch --coverage",
    "lint": "eslint client/**/*.js"
  },
  "dependencies": {
    "bulma": "^0.4.3",
    "font-awesome": "^4.7.0",
    "webpack": "^3.3.0",
    "autoprefixer": "^7.1.2",
    "html-webpack-plugin": "^2.29.0",
    "preload-webpack-plugin": "^1.2.2",
    "compression-webpack-plugin": "^1.0.0",
    "extract-text-webpack-plugin": "^3.0.0"
  },
  "devDependencies": {
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.0",
    "css-loader": "^0.28.4",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "file-loader": "^0.11.2",
    "jest-cli": "^20.0.4",
    "postcss-loader": "^2.0.6",
    "rimraf": "^2.6.1",
    "sinon": "^2.3.8",
    "sinon-as-promised": "^4.0.3",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.9",
    "webpack-dev-server": "^2.5.1"
  },
  "jest": {
    "collectCoverageFrom": [
      "client/**/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "banches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  }
}
