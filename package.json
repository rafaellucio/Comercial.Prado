{
  "name": "comercialprado",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start:server": "hotnode ./server/bin/www",
    "start:client": "http-server ./client -P 3000",
    "test": "npm run lint && nyc --reporter=html --reporter=text mocha $(find . -name '*.test.js' ! -ipath '*node_modules*')",
    "lint": "eslint server/**/*.js && eslint client/**/*.js",
    "coverage": "istanbul cover node_modules/.bin/_mocha -- -R spec $(find . -name '*test.js' ! -ipath '*node_modules*')"
  },
  "dependencies": {
    "body-parser": "~1.16.0",
    "bulma": "^0.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.0",
    "express": "~4.14.1",
    "font-awesome": "^4.7.0",
    "mongoose": "^4.9.0",
    "morgan": "~1.7.0",
    "serve-favicon": "~2.3.2"
  },
  "devDependencies": {
    "chai": "^4.1.0",
    "eslint": "^4.2.0",
    "eslint-config-airbnb": "^15.0.2",
    "eslint-plugin-import": "^2.7.0",
    "hotnode": "0.0.8",
    "http-server": "^0.10.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "sinon": "^2.3.8",
    "sinon-mongoose": "^2.0.2",
    "supertest": "^3.0.0"
  }
}
